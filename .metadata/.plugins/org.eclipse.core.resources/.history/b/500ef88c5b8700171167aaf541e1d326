#include <iostream>
const int nome=10;

int main() {
	int a = 3;
	int b= 5;

	nome=3;

	int c=0;
	c=b; //c=5
	b=a;
	a=c;


	printf("nome= %d\n",nome);
	printf("b= %d",b);

	return 0;
}

