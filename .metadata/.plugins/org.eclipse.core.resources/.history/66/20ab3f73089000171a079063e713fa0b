#include <iostream>
using namespace std;

int main() {

	int numero=0;
	int esponente=0;
	int moltiplicazione=0;
		cout<<"inserisci numero"<<endl;
		cin>>numero;
		cout<<"inserisci esponente"<<endl;
		cin>>esponente;
	for(int i=0;i<esponente;i++)
	{
		moltiplicazione=moltiplicazione *numero;
		cout<<"gay"<<moltiplicazione<<endl;



	}




	return 0;


}
